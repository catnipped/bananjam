function enemy_base(x, y)
   enemy = {}
   add(enemies, enemy)
   enemy.x = x
   enemy.y = y
   enemy.polarity = false
   return enemy
end

function create_enemy_simple(x, y)
   enemy = enemy_base(x, y)
   enemy.hp = 20
   enemy.sprite = 32
   enemy.movement = 0
   return enemy
end

function create_enemy_grunt(x, y)
   enemy = enemy_base(x, y)
   enemy.hp = 30
   enemy.sprite = 64
   enemy.movement = 1
   return enemy
end

function create_enemy_destroyer(x, y)
   enemy = enemy_base(x, y)
   enemy.hp = 50
   enemy.sprite = 65
   enemy.movement = 2
   return enemy
end

